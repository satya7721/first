{
  "extends": ["next", "eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["unused-imports"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "off",
    "unused-imports/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "unused-imports/no-unused-imports": "error"
  }
}
